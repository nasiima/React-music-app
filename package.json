{
  "name": "music-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "10.4.5",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0-beta1",
    "css-loader": "^6.7.1",
